<h1>Teams</h1>
<h3>Manage Users</h3>
<div class="form">
    <form action="/teams/adminTeams" method="POST">
        <label>User</label>
        <select name="userId">

            {{#each usersArray}}
            <option value="{{_id}}">{{username}}</option>
            {{/each}}

        </select>
        <label>Team</label>
        <select name="teamId">

            {{#each teamsArray}}
            <option value="{{_id}}">{{name}}</option>
            {{/each}}

        </select>
        <input type="submit" value="Distribute">
    </form>
</div>